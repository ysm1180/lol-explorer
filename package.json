{
  "description": "A minimal Electron application for build",
  "scripts": {
    "postinstall": "cd app && yarn",
    "start": "cross-env NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node -P script/tsconfig.json script/start.ts",
    "start:prod": "cross-env NODE_ENV=production ts-node -P script/tsconfig.json script/start.ts",
    "dev": "cross-env NODE_ENV=development parallel-webpack --watch --config app/webpack.development.ts",
    "compile:dev": "cross-env NODE_ENV=development parallel-webpack --config app/webpack.development.ts",
    "compile:prod": "cross-env NODE_ENV=production NODE_OPTIONS='--max_old_space_size=4096' parallel-webpack --config app/webpack.production.ts",
    "build:dev": "yarn compile:dev && cross-env NODE_ENV=development ts-node -P script/tsconfig.json script/build.ts",
    "build:prod": "yarn compile:prod && cross-env NODE_ENV=production ts-node -P script/tsconfig.json script/build.ts"
  },
  "repository": "https://github.com/ysm1180/lol-explorer",
  "author": "ysm1180 (ysm1180@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.7",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/electron-packager": "^13.0.0",
    "@types/event-kit": "^2.4.0",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^7.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^4.1.15",
    "@types/webpack": "^4.4.31",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.5",
    "@types/webpack-merge": "^4.1.5",
    "@types/ws": "^6.0.1",
    "electron": "^5.0.0",
    "electron-debug": "^2.2.0",
    "electron-devtools-installer": "^2.2.4"
  },
  "dependencies": {
    "7zip": "^0.0.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "core-js": "3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron-packager": "^13.1.1",
    "event-kit": "^2.5.3",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "file-url": "^3.0.0",
    "fs-extra": "^8.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "parallel-webpack": "^2.3.0",
    "querystring": "^0.2.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.1.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "typescript-tslint-plugin": "^0.3.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.4",
    "webpack-merge": "^4.2.1"
  }
}
